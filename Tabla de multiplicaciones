import java.util.Scanner;

public class Main {
    
    public static void main(String[] args) {
        
        Scanner valor = new Scanner (System.in);
        
        System.out.println("Ingrese un numero para saber sus multiplicaciones: ");
        int n = valor.nextInt();        
        
        for (int i = 0; i <= 10; i++){
            System.out.printf("%d x %d = %d\n",n,i,(n * i));
        }
            
    }

}
